{
	"name": "untis-ics-sync-ui",
	"version": "0.0.1",
	"description": "Small and lightweight frontend for untis-ics-sync.",
	"author": "Luna Simons <luna@bddvlpr.com> (https://bddvlpr.com)",
	"license": "BSD-3-Clause",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write . && eslint . --fix",
		"struct": "openapi-typescript http://localhost:3000/swagger-json --output src/lib/api/spec.ts",
		"start": "vite preview --port 3000 --host"
	},
	"devDependencies": {
		"@skeletonlabs/skeleton": "^1.11.0",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.20.4",
		"@tailwindcss/forms": "^0.5.6",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"openapi-fetch": "^0.7.6",
		"openapi-typescript": "^6.5.5",
		"postcss": "^8.4.24",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.2"
	},
	"type": "module"
}
